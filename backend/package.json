    "@types/faker": "^6.6.9",
    "testcontainers": "^10.2.1",
    "jest-extended": "^4.0.1",
    "ts-node": "^10.9.1"
  },
  "keywords": [
    "receipt",
    "storage",
    "ocr",
    "mobile",
    "api"
  ],
  "author": "Receipt Vault Team",
  "license": "PROPRIETARY"
}
