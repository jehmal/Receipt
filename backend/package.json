{
  "name": "receipt-vault-backend",
  "version": "1.0.0",
  "description": "Forever Receipt Vault - Backend API server",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=\"unit\"",
    "test:integration": "jest --testPathPattern=\"integration\" --runInBand",
    "test:security": "npm audit && snyk test",
    "test:load": "jest --testPathPattern=\"load\" --runInBand",
    "test:e2e": "jest --testPathPattern=\"e2e\" --runInBand",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:security",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "db:migrate": "npm run build && node dist/database/migrate.js",
    "db:seed": "npm run build && node dist/database/seed.js",
    "security:scan": "./security/run-zap-scan.sh",
    "security:audit": "npm audit --audit-level=moderate",
    "monitoring:start": "docker-compose -f src/monitoring/docker-compose.monitoring.yml up -d",
    "monitoring:stop": "docker-compose -f src/monitoring/docker-compose.monitoring.yml down",
    "monitoring:logs": "docker-compose -f src/monitoring/docker-compose.monitoring.yml logs -f",
    "profile:start": "clinic doctor -- node dist/index.js",
    "profile:flame": "clinic flame -- node dist/index.js",
    "profile:bubbleprof": "clinic bubbleprof -- node dist/index.js",
    "load:test": "autocannon -c 10 -d 30 http://localhost:3001/health",
    "metrics:export": "curl -s http://localhost:3001/metrics > metrics-$(date +%Y%m%d-%H%M%S).txt"
  },
  "dependencies": {
    "@fastify/cookie": "^9.4.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/formbody": "^7.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^7.2.0",
    "@fastify/multipart": "^8.3.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/static": "^7.0.4",
    "@google-cloud/vision": "^4.0.2",
    "@types/qrcode": "^1.5.5",
    "@workos-inc/node": "^7.53.0",
    "aws-sdk": "^2.1419.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^4.8.0",
    "crypto": "^1.0.1",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.4.5",
    "elasticsearch": "^16.7.3",
    "exceljs": "^4.4.0",
    "fastify": "^4.28.1",
    "file-type": "^21.0.0",
    "ioredis": "^5.3.2",
    "iron-session": "^8.0.4",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "minio": "^8.0.5",
    "nodemailer": "^6.9.4",
    "pdfkit": "^0.17.1",
    "pg": "^8.13.0",
    "pino-pretty": "^13.0.0",
    "qrcode": "^1.5.4",
    "sharp": "^0.32.6",
    "speakeasy": "^2.0.0",
    "uuid": "^9.0.0",
    "winston": "^3.11.0",
    "axios": "^1.6.0",
    "@fastify/csrf-protection": "^6.4.0",
    "@aws-sdk/client-secrets-manager": "^3.600.0",
    "node-vault": "^0.10.2",
    "workos": "^7.0.0",
    "multer": "^1.4.5-lts.1",
    "bcrypt": "^5.1.1",
    "zod": "^3.22.4",
    "dd-trace": "^5.3.0",
    "@sentry/node": "^7.91.0",
    "@sentry/fastify": "^7.91.0",
    "prom-client": "^15.1.0",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/sdk-node": "^0.45.1",
    "@opentelemetry/instrumentation": "^0.45.1",
    "@opentelemetry/instrumentation-fastify": "^0.33.0",
    "@opentelemetry/instrumentation-http": "^0.45.1",
    "@opentelemetry/instrumentation-pg": "^0.38.0",
    "@opentelemetry/exporter-datadog": "^0.45.0",
    "express-status-monitor": "^1.3.4",
    "clinic": "^12.1.0",
    "autocannon": "^7.12.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.4.5",
    "@types/nodemailer": "^6.4.8",
    "@types/pg": "^8.11.10",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "tsx": "^3.14.0",
    "typescript": "^5.1.6",
    "@types/supertest": "^2.0.16",
    "supertest": "^6.3.3",
    "snyk": "^1.1266.0",
    "nock": "^13.3.3",
